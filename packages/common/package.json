{
  "name": "@furucombo/composable-router-common",
  "version": "0.2.1",
  "description": "Composable Router Common SDK",
  "keywords": [
    "furucombo",
    "composable-router"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dinngo/composable-router-sdk.git",
    "directory": "packages/common"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "format": "yarn sort-package-json",
    "lint": "eslint --fix src",
    "prepublishOnly": "yarn build",
    "test": "mocha",
    "test:e2e": "hardhat test",
    "test:unit": "mocha --recursive src",
    "typechain": "typechain --target ethers-v5 --out-dir src/contracts src/abis/*.json"
  },
  "dependencies": {
    "@types/lodash": "^4.14.194",
    "bignumber.js": "^9.1.1",
    "ethers": "^5.7.2",
    "lodash": "^4.17.21",
    "tiny-invariant": "^1.3.1",
    "type-fest": "^3.9.0"
  },
  "devDependencies": {
    "@furucombo/composable-router-test-helpers": "^0.2.1"
  }
}

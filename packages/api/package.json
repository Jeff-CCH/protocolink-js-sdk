{
  "name": "@composable-router/api",
  "version": "0.1.0",
  "description": "Composable Router API SDK",
  "keywords": [
    "composable-router"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dinngodev/router-sdk.git",
    "directory": "packages/common"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "format": "yarn sort-package-json",
    "lint": "eslint --fix src",
    "prepublishOnly": "yarn build",
    "test": "mocha",
    "test:unit": "mocha --recursive src"
  },
  "dependencies": {
    "@uniswap/permit2-sdk": "^1.2.0"
  },
  "devDependencies": {
    "@composable-router/common": "*",
    "@composable-router/core": "*"
  }
}
